    {
      device = "/dev/mapper/vg00-nix";
      fsType = "xfs";
    };

  fileSystems."/home" =
    {
      device = "/dev/mapper/vg00-nix--home";
      fsType = "xfs";
    };


  fileSystems."/boot" =
    {
      device = "/dev/disk/by-uuid/D4BF-2263";
      fsType = "vfat";
    };

  swapDevices = [ ];

  nix.maxJobs = lib.mkDefault 12;
  powerManagement.cpuFreqGovernor = lib.mkDefault "powersave";
}
